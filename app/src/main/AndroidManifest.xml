<?xml version="1.0" encoding="utf-8" ?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lukas_muenzel.appforerg" android:versionCode="11"
    android:versionName="1.1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CaesarAutomatic" />
        <activity
            android:name=".CaesarAutomaticInfo"
            android:label="@string/title_activity_caesar_automatic_info"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".CaesarNormal" />
        <activity
            android:name=".CaesarNormalInfo"
            android:label="@string/title_activity_caesar_normal_info"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>